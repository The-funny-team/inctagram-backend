{
  "name": "inctagram-backend",
  "version": "0.2.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:gateway": "nest build gateway",
    "build:storage": "nest build storage",
    "build:notifier": "nest build notifier",
    "format:check": "prettier --check \"{apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:gateway": "node dist/apps/gateway/apps/gateway/src/main",
    "start:storage": "node dist/apps/storage/apps/storage/src/main",
    "start:notifier": "node dist/apps/notifier/apps/notifier/src/main",
    "start:dev": "nest start --watch",
    "start:dev:gateway": "cross-env NODE_ENV=development nest start gateway --debug --watch",
    "start:dev:storage": "cross-env NODE_ENV=development nest start storage --debug --watch",
    "start:dev:notifier": "cross-env NODE_ENV=development nest start notifier --debug --watch",
    "start:debug": "nest start --debug --watch",
    "eslint:libs:check": "eslint \"libs/**/*.ts\"",
    "eslint:libs:fix": "eslint \"libs/**/*.ts\" --fix",
    "eslint:gateway:check": "eslint \"apps/gateway/**/*.ts\"",
    "eslint:storage:check": "eslint \"apps/storage/**/*.ts\"",
    "eslint:storage:fix": "eslint \"apps/storage/**/*.ts\" --fix",
    "eslint:notifier:check": "eslint \"apps/notifier/**/*.ts\"",
    "eslint:gateway:fix": "eslint \"apps/gateway/**/*.ts\" --fix",
    "format:libs:check": "prettier --check \"libs/**/*.ts\"",
    "format:libs:fix": "prettier --write \"libs/**/*.ts\"",
    "format:gateway:check": "prettier --check \"apps/gateway/**/*.ts\"",
    "format:storage:check": "prettier --check \"apps/storage/**/*.ts\"",
    "format:storage:fix": "prettier --write \"apps/storage/**/*.ts\"",
    "format:notifier:check": "prettier --check \"apps/notifier/**/*.ts\"",
    "format:gateway:fix": "prettier --write \"apps/gateway/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:libs:check": "yarn eslint:libs:check && yarn format:libs:check",
    "lint:gateway:check": "yarn eslint:gateway:check && yarn format:gateway:check",
    "lint:storage:check": "yarn eslint:storage:check && yarn format:storage:check",
    "lint:notifier:check": "yarn eslint:notifier:check && yarn format:notifier:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:gateway": "NODE_ENV=development jest --watch --config apps/gateway/test/integration.tests/jest-integration.json",
    "test:gateway": "jest --config apps/gateway/jest.json",
    "test:gateway:e2e": "jest --config apps/gateway/test/e2e.tests/jest-e2e.json",
    "test:storage": "jest --config apps/storage/jest.json",
    "test:storage:e2e": "jest --config apps/storage/test/jest-e2e.json",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:storage": "jest --config apps/storage/test/jest-e2e.json",
    "test:e2e:dev": "NODE_ENV=development jest --config apps/gateway/test/e2e.tests/jest-e2e.json",
    "tsc:libs:check": "tsc --p libs/tsconfig.libs.json --noEmit",
    "tsc:gateway:check": "tsc --p apps/gateway/tsconfig.app.json --noEmit",
    "tsc:storage:check": "tsc --p apps/storage/tsconfig.app.json --noEmit",
    "prisma:migrate-create-only": "dotenv -e envs/.gateway.development.env prisma migrate dev --create-only",
    "prisma:migrate": "dotenv -e envs/.gateway.development.env prisma migrate dev",
    "prisma:push": "prisma db push"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/lib-storage": "^3.556.0",
    "@aws-sdk/s3-request-presigner": "^3.556.0",
    "@nestjs-modules/mailer": "1.10.3",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@prisma/client": "^5.12.1",
    "@types/passport-local": "^1.0.38",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "0.10.3",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "mongoose": "^8.3.2",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^5.12.1",
    "qs": "^6.12.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.3",
    "string-width": "^4.2.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleDirectories": [
      "<rootDir>/../",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^@gateway/(.*)$": "<rootDir>/apps/gateway/$1",
      "^libs/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@libs/(.*)$": "<rootDir>/libs/$1",
      "^@fileService/(.*)$": "<rootDir>/apps/storage/$1"
    },
    "testTimeout": 15000
  },
  "prisma": {
    "schema": "./apps/gateway/prisma/schema.prisma"
  }
}
