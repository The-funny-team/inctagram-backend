{
  "moduleFileExtensions": ["js", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testRegex": "^(?!.*\\.e2e\\.spec\\.ts$).*\\.spec\\.ts$",
  "moduleNameMapper": {
    "^@libs/(.*)$": ".<rootDir>/../../libs/$1",
    "^@fileService/(.*)$": "<rootDir>/$1"
  },
  "testTimeout": 15000
}