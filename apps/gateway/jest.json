{
  "moduleFileExtensions": ["js", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testRegex": "^(?!.*\\.e2e\\.spec\\.ts$).*\\.spec\\.ts$",
  "moduleNameMapper": {
    "^@gateway/(.*)$": "<rootDir>/$1",
    "^@app/core/(.*)$": "<rootDir>/../../libs/core/src/$1",
    "^@libs/(.*)$": "<rootDir>/../../libs/$1",
    "^@fileService/(.*)$": "<rootDir>/../inctagram-upload-files/$1"

  },
  "testTimeout": 15000
}